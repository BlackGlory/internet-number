{
  "name": "internet-number",
  "version": "2.2.8",
  "description": "Utilities for fethcing and parsing latest statistics files from RIR",
  "files": [
    "lib"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/internet-number.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.1.12",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.32",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "husky": "^4.3.6",
    "iterable-operator": "^0.13.4",
    "jest": "^26.6.3",
    "memfs": "^3.2.0",
    "nanoid": "^3.1.20",
    "npm-run-all": "^4.1.5",
    "return-style": "^0.12.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.1.1",
    "ts-jest": "^26.5.3",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@blackglory/types": "^0.3.3",
    "get-uri": "^3.0.2",
    "md5-file": "^5.0.0"
  }
}
